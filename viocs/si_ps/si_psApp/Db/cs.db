
record(ai, "SIPS-C$(plane)S-$(sector)$(pos)-RB")
{
    field(DESC, "SI C$(plane)S kick readback sector $(sector) pos $(pos)")
    field(SCAN, ".1 second")
    field(INP, "VA-SIPS-C$(plane)S-$(sector)$(pos) NPP NMS")
    field(EGU, "rad")
    #field(FLNK, "SIPS-C$(plane)S-$(sector)$(pos)-RB-CURRENT")
}

