
record(event, "EVENT-BORF-SP-INIT")
{
    field(VAL, 124)
}

record(ai, "BORF-VOLTAGE-RB")
{
    field(DESC, "BO rf voltage")
    field(SCAN, ".1 second")
    field(PREC, 10)
    field(INP, "VA-BORF-VOLTAGE")
    field(EGU, "V")
}

record(ai, "BORF-FREQUENCY-RB")
{
    field(DESC, "BO rf frequency")
    field(SCAN, ".1 second")
    field(PREC, 10)
    field(INP, "VA-BORF-FREQUENCY")
    field(EGU, "Hz")
}

record(ao, "BORF-FREQUENCY-SP")
{
    field(DESC, "BO rf frequency setpoint")
    field(SCAN, "Passive")
    field(PREC, 10)
    field(OUT, "VA-BORF-FREQUENCY")
    field(EGU, "Hz")
}

record(seq, "SEQ-BORF-FREQUENCY")
{
    field(SCAN, "Event")
    field(EVNT, 124)
    field(DOL1, "VA-BORF-FREQUENCY")
    field(LNK1, "BORF-FREQUENCY-SP")
    field(FLNK, "BORF-FREQUENCY-SP")
}

record(ao, "BORF-VOLTAGE-SP")
{
    field(DESC, "BO rf voltage")
    field(SCAN, "Passive")
    field(PREC, 10)
    field(OUT, "VA-BORF-VOLTAGE")
    field(EGU, "V")
}

record(seq, "SEQ-BORF-VOLTAGE")
{
    field(SCAN, "Event")
    field(EVNT, 124)
    field(DOL1, "VA-BORF-VOLTAGE")
    field(LNK1, "BORF-VOLTAGE-SP")
    field(FLNK, "BORF-VOLTAGE-SP")
}
